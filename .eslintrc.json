{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "ignorePatterns": ["build-utils", "node_modules/", "dist"],
  "rules": {
    "no-var": 1,
    "no-unused-vars": 1,
    "arrow-body-style": 1,
    "camelcase": 1
  },
  "parser": "@babel/eslint-parser"
}
